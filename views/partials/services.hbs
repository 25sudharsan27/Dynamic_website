<section id="services_section" class="py-4" >
        <div class="container" id="services">
            <h1 class="text-center">Services</h1>

            <div class="row mt-5">
            {{!-- first service column --}}

                {{#each service}}
                <div class="col-md-4 py-4">
                    <div class="card py-3">
                        <div class="card-body">
                            <i class="fa-sharp fa-regular fa-rocket-launch"></i>                          
                            <h3 class="card-title">{{this.title}}</h3>
                            <p>{{this.subtitle}}</p>
                            <a href="{{this.clink}}">Check</a>
                            <a href="{{this.slink}}" class="m1-3">Support</a>
                        </div>
                    </div>
                
                </div>
                {{/each}}
                
            </div>
        </div>
    </section>